-- done 0702
d1$
  stack [
  every 5 (# s "~") $
  whenmod 15 12 (# s "~") $
  every 3 (0.2 ~>) $
  listen 1 $
  s "~? rimnewSd([[3 4][3 4]/2],5,2)"
