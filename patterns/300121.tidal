  # envLev2 "10.1 1.8"
  # dcy "0.0001 0.004"
  # amp 0.06
  ,
  foldEvery [6,10] (# s "~") $
  every 3 (0.2 <~) $
  every 9 (density "[1.5 2]")$
  whenmod 8 6 (off 4 (# release 0.2)) $
  listen 1 $
  s "snapkickSd([3 4],[5 10],[2 3])"
  # bdFrqL1 "600 240"
  # bdFrqL2 "250 20 120"
  # clkfFundFreq "600 1200 120"
  # amp 0.3
  # hpf 50
  # comp 0.1
  # wall 1
  # walllev 0.7
  ,
  whenmod 10 1 (# s "~") $
  off 2 (# vowel "[i e] [i o u e]") $
  every 3 (slow 2) $
  every 5 ((# s "~").(slow 2)) $
  listen 1 $
  seqPLoop [
    (0,15, s "sosTomSusSd([4 6],[20 10],[1 3])"
    # drumRel "[0.1 0.4 0.6]"
    # stickRel "[0.01 0.2 0.6 0.8]"
    # freq "[360 240 120 1600?]"
    -- # amp 0.8
    # bpf "1320 480 720 2000 8000"
    # bpq "0.1 0.6 0.5 0.8 0.2"
    )
    ,
    (16,31,  weave 10
      (s "sosTomSusSd([4 6],[5 10],[1 3])")
      [drumRel "[0.1 0.4 0.6]",
      stickRel "[0.01 0.2 0.6 0.8]",
      freq "[360 240 120 1600?]",
      -- # amp 0.8
      bpf "1320 480 720 2000 8000",
      bpq "0.1 0.6 0.5 0.8 0.2"])
    ]
    # amp 0.12
    # sustain 1
  ,
  whenmod 12 2 (# s "~") $
  listen 1 $
  whenmod 6 3 (off 3 ((0.2 <~).(# waveloss "0 12 24 36 48"))) $
  s "beatingSd([2 1 5],10,2)"
  # freq "[12000 9200 8000 10000]/3"
  # beatFreq "[25 20 15]"
  # atk "0.2 [0.4 0]"
  # dcy "0.5 0.9 1"
  # rel "0.1 0.5"
  # amp 0.2
  # sustain "1 1.2 0.8"
  ,
  listen 1 $
  foldEvery [3,7] (listen 0 . slow 2) $
  slow 4 $
  s "triangleWaveBellsSusSd(<[3 6] [5 2]>,10,2) ~"
  # atk "0.01"
  # amp 0.2
  # dcy "0.1 0.2 "
  # sustain "1 0.7"
  # rls "0.5 0.3 0.7"
  # freq "40 360"
  # lforate "10 [25 40]"
  # lfowidth "0.7 0.1 0.7"
  # cutoff "[100 420] 1250"
  # rq  "0.5 0.8"
]
# cps 0.6
|* gain 1.11


